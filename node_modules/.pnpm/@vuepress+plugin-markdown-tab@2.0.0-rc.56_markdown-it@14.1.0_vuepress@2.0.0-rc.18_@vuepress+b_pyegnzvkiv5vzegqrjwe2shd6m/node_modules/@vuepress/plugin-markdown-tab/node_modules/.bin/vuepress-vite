#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/bin/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/bin/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules:/mnt/d/FREETIME_PROJECTS/programuoki_web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.18_@types+node@22.9.1_sass-embedded@1.81.0__vue@3.5.13/node_modules/vuepress/bin/vuepress-vite.js" "$@"
fi
